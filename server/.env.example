# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority

# JWT Secret (Use a long random string)
JWT_SECRET=your-super-secret-random-string-min-32-characters-long

# ImageKit Configuration (Get from https://imagekit.io)
IMAGEKIT_PUBLIC_KEY=your_public_key_here
IMAGEKIT_PRIVATE_KEY=your_private_key_here
IMAGEKIT_URL_ENDPOINT=https://ik.imagekit.io/your_id_here

# Email Configuration (Optional - for booking notifications)
EMAIL_USER=your-gmail@gmail.com
EMAIL_PASS=your-gmail-app-password
OWNER_EMAIL=owner-email@gmail.com

# CORS (Set this to your frontend URL after deploying)
FRONTEND_URL=http://localhost:5173

# Server Port
PORT=3000
